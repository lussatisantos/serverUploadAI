#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/1/OneDrive/Documentos/GitHub/upload-ai-server/node_modules/.pnpm/pino@8.15.1/node_modules/pino/node_modules:/mnt/c/Users/1/OneDrive/Documentos/GitHub/upload-ai-server/node_modules/.pnpm/pino@8.15.1/node_modules:/mnt/c/Users/1/OneDrive/Documentos/GitHub/upload-ai-server/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/Users/1/OneDrive/Documentos/GitHub/upload-ai-server/node_modules/.pnpm/pino@8.15.1/node_modules/pino/node_modules:/mnt/c/Users/1/OneDrive/Documentos/GitHub/upload-ai-server/node_modules/.pnpm/pino@8.15.1/node_modules:/mnt/c/Users/1/OneDrive/Documentos/GitHub/upload-ai-server/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../pino@8.15.1/node_modules/pino/bin.js" "$@"
else
  exec node  "$basedir/../../../../../pino@8.15.1/node_modules/pino/bin.js" "$@"
fi
